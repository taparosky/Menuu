@model SnackListViewModel

<head>
    <link rel="stylesheet" href="~/css/site.css" type="text/css" />
</head>

<div>
    <h2>@Model.CurrentCategory</h2>
</div>

<div class="container">
    <div class="row">
        @foreach (var snack in Model.Snacks) 
        {
            <partial name="_SnacksSummary" for="@snack"/>
        }
    </div>
</div>
